@page "/"
@using Syncfusion.Blazor.Kanban
<SfKanban TValue="TasksModel" KeyField="Company" DataSource="Tasks">
    <KanbanColumns>
        @foreach (var company in Companies) {
            <KanbanColumn HeaderText="@company" KeyField="@(new List<string>() {company})">
            </KanbanColumn>
        }

    </KanbanColumns>
    <KanbanCardSettings HeaderField="Title" ContentField="Summary"></KanbanCardSettings>
    <KanbanSwimlaneSettings KeyField="StatusRank"></KanbanSwimlaneSettings>
</SfKanban>

